<?xml version="1.0" encoding="UTF-8"?>
<clusters>
     	<!-- first cluster Maternal weight less than 40 -->	
	<clusterSet>
	
			
	        <id>1</id>
		    <name><![CDATA[<40]]> </name>
		        
			    
				<ruleSet>
                       <predicate>lt</predicate>					
					   <expression>
					         <question>weight</question>
					         <edge>40</edge>
					   </expression>
					   <conjunction>end</conjunction>
				</ruleSet>
	    

	</clusterSet>
	

		        
